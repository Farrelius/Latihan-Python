# PROGRAM RUMUS LIMAS SEGIEMPAT BY FARREL
# 20/10/2024 14:37

def limas_segiempat():

# AMBIL DATA DARI INPUT USER 

    LS1 = int(input("Masukkan Luas Sisi Limas Segiempat ke 1 : "))
    LS2 = int(input("Masukkan Luas Sisi Limas Segiempat ke 2 : "))
    LS3 = int(input("Masukkan Luas Sisi Limas Segiempat ke 3 : "))
    LS4 = int(input("Masukkan Luas Sisi Limas Segiempat ke 4 : "))
    LS5 = int(input("Masukkan Luas Sisi Limas Segiempat ke 5 : "))
    LA = int(input("Masukkan Luas Alas Limas Segiempat : "))
    T = int(input("Masukkan Tinggi Limas Segiempat : "))

# ALGORITMA

    L = lambda LS1,LS2,LS3,LS4,LS5:  LS1+LS2+LS3+LS4+LS5
    V = lambda LA,T : 1/3*LA*T

    print(f'''LUAS LIMAS SEGIEMPAT ADALAH = {L(LS1,LS2,LS3,LS4,LS5)}
VOLUME LIMAS SEGIEMPAT ADALAH = {V(LA,T)}''')
    
limas_segiempat()